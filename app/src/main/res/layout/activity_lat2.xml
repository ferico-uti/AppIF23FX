<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Lat2Activity">

    <Button
        android:id="@+id/btn_menu1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menu 1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_menu2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menu 2"
        app:layout_constraintBottom_toBottomOf="@+id/btn_menu1"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn_uti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UTI"
        app:layout_constraintBottom_toBottomOf="@+id/btn_menu1"
        app:layout_constraintEnd_toStartOf="@+id/btn_menu2"
        app:layout_constraintStart_toEndOf="@+id/btn_menu1" />
</androidx.constraintlayout.widget.ConstraintLayout>